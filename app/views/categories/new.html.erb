<%= render "shared/content_title", title: "New Category"%>
<%= render "shared/errors", obj: @category %>

<div class="well">
  <%= form_for @category do |f| %>
    <div class="control_group">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <%= f.submit "Create category", class: "btn btn-primary" %>
  <% end %>
</div>